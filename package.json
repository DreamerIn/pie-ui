{
  "name": "@pie-ui/pie-ui",
  "version": "1.0.0",
  "description": "the pie-elements mono repo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "release": "npm run clean && npm test && npm run build && lerna publish --conventional-commits --yes",
    "release:next": "npm run clean && npm test && npm run build && lerna publish --canary --yes --conventional-commits --preid next --dist-tag next",
    "lint": "lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/eslint  --ignore-path \\$LERNA_ROOT_PATH/.eslintignore --ext .js --ext .jsx src",
    "clean": "lerna exec -- rm -fr lib",
    "build": "npm run clean && npm run lint && lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/babel --ignore '/__test__/','/__tests__/' src -d lib --copy-files --source-maps",
    "build:demo": "scripts/build-demo",
    "deploy:demo:dev": "scripts/deploy-demo --alias pie-ui-develop",
    "deploy:demo": "scripts/deploy-demo --alias pie-ui",
    "build-release-deploy": "scripts/build-release-deploy"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@pie-framework/build-helper": "^1.2.1",
    "@pie-lib/test-utils": "0.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "debug": "^3.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^7.0.1",
    "jest": "^22.4.3",
    "lerna": "^3.11.0",
    "minimist": "^1.2.0",
    "now": "^13.1.3",
    "opn": "^5.4.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
