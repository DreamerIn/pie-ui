{
  "name": "@pie-ui/pie-ui",
  "version": "1.0.0",
  "description": "the pie-elements mono repo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "lerna run install",
    "test": "jest",
    "release":
      "npm test && npm run build && lerna publish --conventional-commits",
    "lint":
      "lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/eslint  --ignore-path \\$LERNA_ROOT_PATH/.eslintignore --ext .js --ext .jsx src",
    "clean": "lerna exec -- rm -fr lib",
    "build":
      "npm run clean && npm run lint && lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/babel --ignore '/__test__/','/__tests__/' src -d lib --copy-files"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "lerna": "^2.11.0",
    "jest": "^22.4.3",
    "babel-jest": "^22.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-core": "^6.26.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "react": "^16.3.2",
    "react-dom": "16.3.2",
    "minimist": "^1.2.0"
  }
}
