#!/usr/bin/env bash

echo "GIT before release"
git status 
git diff 
git log -3
git remote --v

echo "checkout TRAVIS_BRANCH"

git checkout ${TRAVIS_BRANCH}
git status 
git rev-parse --short HEAD
git log -3

